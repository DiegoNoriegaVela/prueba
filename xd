cd \ruta\al\proyecto\gss-crypto-tool
mkdir target\test_classes 2>nul

rem Compilar el test contra tu jar y las libs externas
javac -encoding UTF-8 ^
  -cp "target\gss-crypto-tool.jar;target\lib\*" ^
  -d target\test_classes ^
  test\TestLibGSS.java

java -cp "target\test_classes;target\gss-crypto-tool.jar;target\lib\*" TestLibGSS

java -cp "target\test_classes;target\gss-crypto-tool.jar;target\lib\*" com.scotiabank.gssvault.test.TestLibGSS
